package  com.examly.springapp.controller;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.web.bind.annotation.CrossOrigin;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.PostMapping;
import org.springframework.web.bind.annotation.RequestBody;
import org.springframework.web.bind.annotation.RestController;

import com.review.feedback.model.feedback;
import com.review.feedback.service.FeedbackService;

@RestController
@CrossOrigin(allowedHeaders = "*",origins = "*" )
public class FeedbackController {

	@Autowired
	private FeedbackService feedbackService;
	
	@PostMapping("/registerFeedback")
	
	public feedback registerFeedback(@RequestBody feedback feedback) {
		return feedbackService.registerFeedback(feedback);
	}
	@GetMapping("/viewfeedback")
	public List<feedback> viewfeedback(){
		return feedbackService.viewfeedback();
			
		
	}
}
